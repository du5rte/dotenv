{
  "name": "dotenv",
  "version": "4.0.0",
  "description": "Loads environment variables from .env file",
  "main": "lib/main.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rm -rf lib",
    "dev": "npm run build -- --watch & npm run test -- --watch",
    "prepublish": "npm run clean && npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/motdotla/dotenv.git"
  },
  "keywords": [
    "dotenv",
    "env",
